    "lineWidth": 80,
    "css.enabled": true,
    "css.indentWidth": 4
  },
  "yaml": {},
  "excludes": [
    "**/*-lock.json",
    "**/node_modules"
  ],
  "includes": [
    "**/*.{md,mdx,yml,ts,mjs,md,mdx,css,astro,yaml,json}"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.19.3.wasm",
    "https://plugins.dprint.dev/markdown-0.17.2.wasm",
    "https://plugins.dprint.dev/biome-0.6.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.0.wasm"
  ]
}
